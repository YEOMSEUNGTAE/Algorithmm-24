    "def factorial_recur(n):\n",
    "    if n == 1:\n",
    "        return 1\n",
    "    else:\n",
    "        return n * factorial_recur(n - 1)\n",
    "\n",
    "print(f\"10!ëŠ” {factorial_recur(10)}\")"
   ]
